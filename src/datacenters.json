                "title": "Primary Use",
                "type": "string",
                "enum": [
                  "Electrical Design",
                  "Mechanical Design",
                  "General Construction"
                ]
              },
              "Years Built": {
                "type": "array",
                "format": "grid",
                "title": "Year(s) Built",
                "uniqueItems": true,
                "items": {
                  "type": "integer",
                  "minimum": 1950,
                  "maximum": 2050
                }
              },
              "Years renovated": {
                "type": "array",
                "format": "tab",
                "title": "Year(s) Renovated",
                "uniqueItems": true,
                "items": {
                  "type": "integer",
                  "minimum": 1950,
                  "maximum": 2050
                }
              },
              "Renovation comments": {
                "type": "string",
                "default": "Not applicable",
                "minLength": 2,
                "maxLength": 280
              },
