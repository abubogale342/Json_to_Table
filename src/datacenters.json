    },
    "Ownership Info": {
      "type": "object",
      "title": "Ownership Information",
      "properties": {
        "Owner/Landlord": {
          "type": "string",
          "title": "Owner/Landlord: ",
          "minLength": 4
        },
        "entity type": {
          "title": "Type of Entity: ",
          "type": "string",
          "enum": [
            "C-Corp",
            "S-Corp",
            "REIT",
            "Other"
          ]
        }
      }
    },
    "Transaction Info": {
      "type": "object",
      "title": "Transaction Information",
      "properties": {
        "Currency": {
          "type": "string",
          "title": "Currency: ",
          "enum": [
            "USD",
            "GBP",
            "EUR",
            "CNY",
            "JPY",
            "AUD"
          ]
        },
        "Market Rent": {
          "type": "number",
          "title": "Market Rent: ",
          "minimum": 1,
          "maximum": 1000000,
          "description": "Average price per unit of measurement"
        },
        "Sale Price": {
          "type": "number",
          "title": "Sale Price: ",
          "minimum": 1,
          "maximum": 1000000000
        },
        "Sale Date": {
          "type": "string",
          "title": "Sale Date: ",
          "default": "YYYY-MM-DD",
          "minLength": 10,
          "maxLength": 10
        },
        "Appraisal Value": {
          "type": "number",
          "title": "Appraisal Value: ",
          "minimum": 1,
          "maximum": 1000000000
        },
        "Appraisal Date": {
          "type": "string",
          "title": "Appraisal Date: ",
          "default": "YYYY-MM-DD",
          "minLength": 10,
          "maxLength": 10
        },
        "Deal Structure": {
          "type": "string",
          "title": "Deal Structure: ",
          "enum": [
            "Cash Consideration",
            "Stock as Consideration",
            "Issuance of New Debt",
            "Pre-existing Debt",
            "Convertible Debt"
          ]
        },
        "Transaction Type": {
          "type": "string",
          "title": "Transaction Type: ",
          "enum": [
            "Taxable",
            "Non-taxable"
          ]
        },
        "Cap Rate": {
          "type": "number",
          "title": "Cap Rate: ",
          "minimum": 0,
          "maximum": 100,
          "description": "Number expressed as a percentage, e.g. 7.5% as 7.5"
        },
        "Notes": {
          "type": "string",
          "title": "Notes: "
        }
      }
    },
    "leases": {
      "type": "array",
      "format": "grid",
      "title": "Leases",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "title": "Lease",
        "format": "grid",
        "properties": {
          "Tenant": {
            "type": "string",
            "description": "Tenant/Leasee",
            "minLength": 4
          },
          "Size": {
            "type": "object",
            "properties": {
              "Unit of Measurement": {
                "type": "string",
                "enum": [
                  "Square Foot",
                  "Square Meter"
                ]
              },
              "Space Leased": {
                "type": "number",
                "description": "Quantity of Space Leased",
                "minimum": 10,
                "maximum": 1000000
              }
            }
          },
          "Rent": {
            "type": "object",
            "properties": {
              "Currency": {
                "type": "string",
                "enum": [
                  "USD",
                  "GBP",
                  "EUR",
                  "CNY",
                  "JPY",
                  "AUD"
                ]
              },
              "Amount": {
                "type": "number",
                "description": "Monthly rent under the lease",
                "minimum": 10,
                "maximum": 1000000
              }
            }
          },
          "Lease Start Date": {
            "type": "string",
            "description": "Date the tenant's lease commenced",
            "default": "YYYY-MM-DD",
            "minLength": 10,
            "maxLength": 10
          },
          "Lease End/Revision Date": {
            "type": "string",
            "description": "Date the tenant's lease is set to end or rent will reset",
            "default": "YYYY-MM-DD",
            "minLength": 10,
            "maxLength": 10
          },
          "Termination Terms": {
            "type": "string",
            "enum": [
              "Existing Break Clause",
              "No Break Clause"
            ]
          },
          "Percentage Data Center": {
            "type": "number",
            "minimum": 0,
            "maximum": 100,
            "description": "Number expressed as a percentage, e.g. 80% as 80"
          }
        }
      }
    },
    "Operating Info": {
      "type": "object",
      "properties": {
        "Property": {
          "type": "array",
          "format": "grid",
          "title": "Operating Information",
          "uniqueItems": true,
          "items": {
            "type": "object",
            "title": "Building",
            "properties": {
              "Building ID": {
                "type": "string",
                "description": "Building Identifier (Name, ID#, Address, etc.)"
              },
              "Currency": {
                "type": "string",
                "enum": [
                  "USD",
                  "GBP",
                  "EUR",
                  "CNY",
                  "JPY",
                  "AUD"
                ]
              },
              "Year": {
                "type": "integer",
                "minimum": 1950,
                "maximum": 2050
              },
              "Payroll": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Payroll costs for the year"
              },
              "Repairs and Maintenance": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Repairs and maintenance for the year"
              },
              "Property Taxes": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Repairs and maintenance for the year"
              },
              "Utilities": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Utility costs for the year"
              },
              "Property Insurance Cost": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Property insurance costs for the year"
              },
              "Cleaning Cost": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Cleaning costs for the year"
              },
              "Capex Cost": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Capex for the year"
              },
              "Management Fee": {
                "type": "number",
                "minimum": 1,
                "maximum": 1000000,
                "description": "Management Fee for the year"
              }
            }
          }
        },
        "Research & Development Cost": {
          "type": "number",
          "minimum": 1,
          "maximum": 1000000,
          "description": "R&D for the year"
        },
        "Sales & Marketing Cost": {
          "type": "number",
          "minimum": 1,
          "maximum": 1000000,
          "description": "S&M for the year"
        },
        "Customer Information": {
          "type": "object",
          "properties": {
            "Average Contract Period": {
              "type": "number",
              "minimum": 1,
              "maximum": 120,
              "description": "Average Length of Customer Contracts (Months)"
            },
            "Early Termination Details": {
              "type": "string"
            },
            "Key Customer Exposure": {
              "type": "array",
              "format": "grid",
              "uniqueItems": true,
              "items": {
                "type": "object",
                "description": "Customers with over 20% revenue exposure",
                "properties": {
                  "Customer Name": {
                    "type": "string",
                    "minLength": 3,
                    "maxLength": 100
                  },
                  "Customer Revenue Exposure Percentage": {
                    "type": "number",
                    "minimum": 20,
                    "maximum": 100
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}